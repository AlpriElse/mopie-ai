   "source": [
    "import boto3\n",
    "import os\n",
    "\n",
    "s3 = boto3.resource('s3')\n",
    "\n",
    "\n",
    "for filename in os.listdir('downloads'):\n",
    "  s3.meta.client.upload_file(\n",
    "    f'downloads/{filename}', \n",
    "    'mopie-video-metadata', \n",
    "    filename\n",
    "  )\n",
    "\n"
   ]
